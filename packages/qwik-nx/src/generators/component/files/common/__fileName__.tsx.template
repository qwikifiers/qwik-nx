import { component$<%= hasStyles ? ', useStylesScoped$' : '' %> } from '@builder.io/qwik';
<% if(hasStyles) { %>
import styles from './<%- fileName %>.<%- style %>?inline';
<% } %>

export const <%= className %> = component$(()=>{
  <% if(hasStyles) { %>
  useStylesScoped$(styles);
  <% } %>
  return <>
    <%= className %> works!
  </>;
});